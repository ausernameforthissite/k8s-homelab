        "iconColor": "red",
        "name": "Node reboot",
        "target": {
          "expr": "changes(node_boot_time_seconds{instance=\"$instance\"}[$__interval]) > 0",
          "refId": "Anno"
        },
        "textFormat": "{{instance}}",
