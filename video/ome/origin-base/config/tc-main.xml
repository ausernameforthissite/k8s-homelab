
<Application>
    <Name>tc</Name>
    <Type>live</Type>
    <OutputProfiles>
        <HardwareAcceleration>true</HardwareAcceleration>
        <OutputProfile>
            <Name>goparse!name{${env:OME_SERVER_NAME:Unknown OME server}}prefer{opus_bypass}preferType{webrtc}</Name>
            <OutputStreamName>${OriginStreamName}</OutputStreamName>

            <!-- Note that the keywords "playlist" and "chunklist" MUST NOT be included in FileName. -->
            <Playlist>
                <Name>Low Latency</Name>
                <FileName>opus_multi</FileName>
                <Options>
                    <WebRtcAutoAbr>false</WebRtcAutoAbr>
                </Options>
                <Rendition>
                    <Name>720p</Name>
                    <Video>720p</Video>
                    <Audio>opus</Audio>
                </Rendition>
                <Rendition>
                    <Name>1080p</Name>
                    <Video>1080p</Video>
                    <Audio>opus</Audio>
                </Rendition>
                <Rendition>
                    <Name>Bypass</Name>
                    <Video>bypass_video</Video>
                    <Audio>opus</Audio>
                </Rendition>
            </Playlist>
            <Playlist>
                <Name>Low Latency</Name>
                <FileName>opus_720p</FileName>
                <Options>
                    <WebRtcAutoAbr>false</WebRtcAutoAbr>
                </Options>
                <Rendition>
                    <Name>720p</Name>
                    <Video>720p</Video>
                    <Audio>opus</Audio>
                </Rendition>
            </Playlist>
            <Playlist>
                <Name>Low Latency</Name>
                <FileName>opus_1080p</FileName>
                <Options>
                    <WebRtcAutoAbr>false</WebRtcAutoAbr>
                </Options>
                <Rendition>
                    <Name>1080p</Name>
                    <Video>1080p</Video>
                    <Audio>opus</Audio>
                </Rendition>
            </Playlist>
            <Playlist>
                <Name>Low Latency</Name>
                <FileName>opus_bypass</FileName>
                <Options>
                    <WebRtcAutoAbr>false</WebRtcAutoAbr>
                </Options>
                <Rendition>
                    <Name>Bypass</Name>
                    <Video>bypass_video</Video>
                    <Audio>opus</Audio>
                </Rendition>
            </Playlist>
            <Playlist>
                <Name>Latency 3s</Name>
                <FileName>aac_multi</FileName>
                <Rendition>
                    <Name>720p</Name>
                    <Video>720p</Video>
                    <Audio>aac</Audio>
                </Rendition>
                <Rendition>
                    <Name>1080p</Name>
                    <Video>1080p</Video>
                    <Audio>aac</Audio>
                </Rendition>
                <Rendition>
                    <Name>Bypass</Name>
                    <Video>bypass_video</Video>
                    <Audio>aac</Audio>
                </Rendition>
            </Playlist>
            <Playlist>
                <Name>Latency 3s</Name>
                <FileName>aac_720p</FileName>
                <Rendition>
                    <Name>720p</Name>
                    <Video>720p</Video>
                    <Audio>aac</Audio>
                </Rendition>
            </Playlist>
            <Playlist>
                <Name>Latency 3s</Name>
                <FileName>aac_1080p</FileName>
                <Rendition>
                    <Name>1080p</Name>
                    <Video>1080p</Video>
                    <Audio>aac</Audio>
                </Rendition>
            </Playlist>
            <Playlist>
                <Name>Latency 3s</Name>
                <FileName>aac_bypass</FileName>
                <Rendition>
                    <Name>Bypass</Name>
                    <Video>bypass_video</Video>
                    <Audio>aac</Audio>
                </Rendition>
            </Playlist>

            <Encodes>
                <Video>
                    <Name>bypass_video</Name>
                    <Bypass>true</Bypass>
                </Video>
                <Video>
                    <Name>720p</Name>
                    <Codec>h264</Codec>
                    <Width>1280</Width>
                    <Height>720</Height>
                    <Bitrate>${env:OME_720_BITRATE:1500000}</Bitrate>
                    <Framerate>60.0</Framerate>
                    <KeyFrameInterval>60</KeyFrameInterval>
                    <BFrames>0</BFrames>
                    <Profile>high</Profile>
                    <Preset>${env:OME_720_PRESET:fast}</Preset>
                    <ThreadCount>4</ThreadCount>
                </Video>
                <Video>
                    <Name>1080p</Name>
                    <Codec>h264</Codec>
                    <Width>1920</Width>
                    <Height>1080</Height>
                    <Bitrate>${env:OME_1080_BITRATE:5000000}</Bitrate>
                    <Framerate>60.0</Framerate>
                    <KeyFrameInterval>60</KeyFrameInterval>
                    <BFrames>0</BFrames>
                    <Profile>high</Profile>
                    <Preset>${env:OME_1080_PRESET:fast}</Preset>
                    <ThreadCount>4</ThreadCount>
                </Video>
                <Image>
                    <Codec>jpeg</Codec>
                    <Height>480</Height>
                    <Framerate>1</Framerate>
                </Image>
                <Audio>
                    <Name>aac</Name>
                    <Codec>aac</Codec>
                    <Bitrate>128000</Bitrate>
                    <Samplerate>48000</Samplerate>
                    <Channel>2</Channel>
                    <BypassIfMatch>
                        <Codec>eq</Codec>
                        <Samplerate>lte</Samplerate>
                        <Channel>eq</Channel>	
                    </BypassIfMatch>
                </Audio>
                <Audio>
                    <Name>opus</Name>
                    <Codec>opus</Codec>
                    <Bitrate>128000</Bitrate>
                    <Samplerate>48000</Samplerate>
                    <Channel>2</Channel>
                    <BypassIfMatch>
                        <Codec>eq</Codec>
                        <Samplerate>lte</Samplerate>
                        <Channel>eq</Channel>	
                    </BypassIfMatch>
                </Audio>
            </Encodes>
        </OutputProfile>
    </OutputProfiles>
    <Providers>
        <OVT />
        <WebRTC>
            <Timeout>30000</Timeout>
            <CrossDomains>
                <Url>*</Url>
            </CrossDomains>
        </WebRTC>
        <RTMP />
        <SRT />
        <RTSPPull />
    </Providers>
    <Publishers>
        <AppWorkerCount>1</AppWorkerCount>
        <StreamWorkerCount>8</StreamWorkerCount>
        <OVT />
        <WebRTC>
            <Timeout>30000</Timeout>
            <Rtx>false</Rtx>
            <Ulpfec>false</Ulpfec>
            <JitterBuffer>false</JitterBuffer>
        </WebRTC>
        <LLHLS>
            <OriginMode>true</OriginMode>
            <!--
            <CacheControl>
                <MasterPlaylistMaxAge>0</MasterPlaylistMaxAge>
                <ChunklistMaxAge>0</ChunklistMaxAge>
                <ChunklistWithDirectivesMaxAge>60</ChunklistWithDirectivesMaxAge>
                <SegmentMaxAge>-1</SegmentMaxAge>
                <PartialSegmentMaxAge>-1</PartialSegmentMaxAge>
            </CacheControl>
            -->
            <ChunkDuration>0.5</ChunkDuration>
            <!-- PartHoldBack SHOULD be at least three times the Part Target Duration -->
            <PartHoldBack>1.5</PartHoldBack>
            <SegmentDuration>6</SegmentDuration>
            <SegmentCount>10</SegmentCount>
            <CrossDomains>
                <Url>*</Url>
            </CrossDomains>
        </LLHLS>
        <Thumbnail>
            <CrossDomains>
                <Url>*</Url>
            </CrossDomains>
        </Thumbnail>
    </Publishers>
</Application>
